<?xml version="1.0" encoding="utf-8" ?>
<ModelFragment xmlns="http://schemas.microsoft.com/LightSwitch/2010/xaml/model"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Application Name="LightSwitchApplication"
               Version="1.0.0.0"
               Shell=":Standard"
               Theme=":Blue"
               DefaultNavigationItem="!module/NavigationItems[Tasks]">
    <Application.Methods>
      <ApplicationMethod Name="ShowLocationsScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="LocationsScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
      <ApplicationMethod Name="ShowProceduresScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="ProceduresScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
      <ApplicationMethod Name="ShowProcedureDetailsScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="ProcedureDetailsScreen" />
        </ApplicationMethod.Attributes>
        <ApplicationMethodParameter Name="ProcedureprocedureId"
                                    ParameterType=":Int32" />
      </ApplicationMethod>
      <ApplicationMethod Name="ShowConfigurationsScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="ConfigurationsScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
      <ApplicationMethod Name="ShowDevicesScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="DevicesScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
      <ApplicationMethod Name="ShowLogsScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="LogsScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
      <ApplicationMethod Name="ShowConnectionsScreen">
        <ApplicationMethod.Attributes>
          <ShowScreenMethod TargetScreen="ConnectionsScreen" />
        </ApplicationMethod.Attributes>
      </ApplicationMethod>
    </Application.Methods>
    <Application.Attributes>
      <AutomaticDisplayName Expression="(\p{Ll})(\p{Lu})|_+"
                            Replacement="$1 $2" />
    </Application.Attributes>
    <Application.NavigationItems>
      <ApplicationNavigationGroup Name="Tasks"
                                  DefaultItem="!module/NavigationItems[Tasks]/Children[DevicesScreen]">
        <ApplicationNavigationGroup.Attributes>
          <DisplayName Value="Setup" />
        </ApplicationNavigationGroup.Attributes>
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowConfigurationsScreen]"
                                   Name="ConfigurationsScreen" />
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowDevicesScreen]"
                                   Name="DevicesScreen" />
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowLocationsScreen]"
                                   Name="LocationsScreen" />
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowProceduresScreen]"
                                   Name="ProceduresScreen" />
      </ApplicationNavigationGroup>
      <ApplicationNavigationGroup Name="group">
        <ApplicationNavigationGroup.Attributes>
          <DisplayName Value="Logs" />
        </ApplicationNavigationGroup.Attributes>
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowConnectionsScreen]"
                                   Name="link1" />
        <ApplicationNavigationLink Command="!module/Commands[ScreenCommandShowLogsScreen]"
                                   Name="link" />
      </ApplicationNavigationGroup>
      <ApplicationNavigationGroup Name="Administration">
        <ApplicationNavigationGroup.Attributes>
          <DisplayName Value="Administration" />
        </ApplicationNavigationGroup.Attributes>
        <ApplicationNavigationLink Command="Microsoft.LightSwitch.Security:!module/Commands[ScreenCommandShowRolesScreen]"
                                   Name="RolesScreen" />
        <ApplicationNavigationLink Command="Microsoft.LightSwitch.Security:!module/Commands[ScreenCommandShowUsersScreen]"
                                   Name="UsersScreen" />
      </ApplicationNavigationGroup>
    </Application.NavigationItems>
    <ApplicationCommand Name="ScreenCommandShowLocationsScreen"
                        Target="!module/Methods[ShowLocationsScreen]" />
    <ApplicationCommand Name="ScreenCommandShowProceduresScreen"
                        Target="!module/Methods[ShowProceduresScreen]" />
    <ApplicationCommand Name="ScreenCommandShowConfigurationsScreen"
                        Target="!module/Methods[ShowConfigurationsScreen]" />
    <ApplicationCommand Name="ScreenCommandShowDevicesScreen"
                        Target="!module/Methods[ShowDevicesScreen]" />
    <ApplicationCommand Name="ScreenCommandShowLogsScreen"
                        Target="!module/Methods[ShowLogsScreen]" />
    <ApplicationCommand Name="ScreenCommandShowConnectionsScreen"
                        Target="!module/Methods[ShowConnectionsScreen]" />
  </Application>

